<h1>ZoomLink</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>week</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= @DAT_OF_WEEK[list.week.to_i] %></td>
        <td><%= link_to list.subject, list.content, { :class => "text-decoration", :target => "_blank"}  %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete,remote: true, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List', new_list_path, class: "btn btn-outline-secondary" %>
