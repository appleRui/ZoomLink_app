<thead>
    <tr>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<tbody>
<% @today_list.each do |toli| %>
<tr>
<td><%= link_to toli.subject, toli.content, { :class => "text-decoration", :target => "_blank"}  %></td>
<tr>
<% end %>
